{
  "name": "discord-bot",
  "version": "1.0.1",
  "type": "module",
  "main": "dist/bot.js",
  "scripts": {
    "start": "tsc && node dist/bot.js",
    "build": "tsc",
    "dev": "node --loader ts-node/esm --watch src/bot.ts",
    "lint": "eslint --ext .ts . --fix",
    "test": "node test/test-bot.js",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "dependencies": {
    "@ai-sdk/google": "^1.1.16",
    "ai": "^4.1.45",
    "axios": "^1.7.9",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "node-ipinfo": "^4.0.1",
    "resend": "^4.1.2",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
